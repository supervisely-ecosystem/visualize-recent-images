{
  "type": "app",
  "version": "2.0.0",
  "name": "Visualize Image Updates",
  "description": "Application that visualizes the most recently updated images",
  "docker_image": "supervisely/base-py-sdk:6.73.91",
  "categories": [
    "images",
    "visualization"
  ],
  "icon": "",
  "icon_cover": true,
  "headless": false,
